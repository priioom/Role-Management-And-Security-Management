<configuration>
    <system.web>
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
   
        <pages enableEventValidation="false"/>
      <customErrors mode="ON" defaultRedirect="~/UI/403.aspx"></customErrors>
    </system.web>
  <system.webServer>
    <staticContent>
      <remove fileExtension=".woff2" />
      <mimeMap fileExtension=".woff2"
               mimeType="font/woff2" />
    </staticContent>


  </system.webServer>
  
</configuration>
